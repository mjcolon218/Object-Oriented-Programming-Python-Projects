def main():
    print('This Program converts from Farenheit to Celsius.')
    Farenheit = int(input(" ITS REALLY NICE OUTSIDE WHATS THE TEMP?:"))
    temperture =(Farenheit - 32) *.5556
    print("its February and its :",temperture,"degrees holy crap")
main()
